print("Fibonacci sequence");

fn fibonacci(n: int): int {
   when n < 2 {
      n
   } else {
      fibonacci(n - 1) + fibonacci(n - 2)
   }
}

i := 0;
nterms := 10;
for i < nterms {
   print("i: " + i + ": " + fibonacci(i));
   i = i + 1;
}

print("");
print("Factorial sequence");

fn factorial(n: int): int {
   when n == 0 {
      1
   } else {
      n * factorial(n - 1)
   }
}

i = 1;
nterms = 5;
for i < nterms {
   print("i: " + i + ": " + factorial(i));
   i = i + 1;
}
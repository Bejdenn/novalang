// string concat
string: str;
string = "There are " + 2 + " dogs.";
str::print("string: " + string);

number: int;
number = 2 + 2;
str::print("number: " + number);

x := 1;
y := 2;
boolean: bool;
str::print("x: " + x + ", y: " + y);

// equals
boolean = "Hallo" == "Hallo";
str::print("Hallo == Hallo: " + boolean);

// not equal
boolean = "1" != "Hallo";
str::print("1 == Hallo: " + boolean);

// less than
boolean = x < y;
str::print("x < y: " + boolean);

// greater than
boolean = x > y;
str::print("x > y: " + boolean);

// less than or equal
boolean = x <= y;
str::print("x <= y: " + boolean);

// greater than or equal
boolean = x >= y;
str::print("x >= y: " + boolean);

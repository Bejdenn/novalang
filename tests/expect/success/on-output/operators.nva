// string concat
string: str;
string = "There are " + str(2) + " dogs.";
str::print("string: " + string);

number: int;
number = 2 + 2;
str::print("number: " + str(number));

x := 1;
y := 2;
boolean: bool;
str::print("x: " + str(x) + ", y: " + str(y));

// equals
boolean = "Hallo" == "Hallo";
str::print("Hallo == Hallo: " + str(boolean));

// not equal
boolean = "1" != "Hallo";
str::print("1 == Hallo: " + str(boolean));

// less than
boolean = x < y;
str::print("x < y: " + str(boolean));

// greater than
boolean = x > y;
str::print("x > y: " + str(boolean));

// less than or equal
boolean = x <= y;
str::print("x <= y: " + str(boolean));

// greater than or equal
boolean = x >= y;
str::print("x >= y: " + str(boolean));

fn swap(a: int[], x: int, y:int): int[] {
    tmp: int;
    tmp = a[x];
    a[x] = a[y];
    a[y] = tmp;

    a
}

input: int[];
input = read_ints();

fn permute(a: int[], l: int, r: int): void {
    if l == r {
        print_arr(a);
    } else {
        i := l;
        for i <= (r - 1) {
            a = swap(a, l, i);
            permute(a, l + 1, r);
            a = swap(a, l, i);
            i = i + 1;
        }
   }
}

